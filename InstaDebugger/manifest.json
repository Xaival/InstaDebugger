{
	"manifest_version": 3,
	"version": "1.0",
	"name": "InstaDebugger",
	"description": "Pausa la página con un alert al momento de abrirse. De esta forma se pueden inspeccionar las webs de redireccionamiento automático.",
	"author": "Xaival",
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content_scripts.js"]
	}],
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png"
		}
	}
}